Import('env')
Import('env_modules')

env_modules.add_source_files(env.modules_sources,"*.c")
env_modules.add_source_files(env.modules_sources,"*.cpp")
env_modules.add_source_files(env.modules_sources,"goap/*.c")
env_modules.add_source_files(env.modules_sources,"goap/*.cpp")

Export('env_modules')
Export('env')
